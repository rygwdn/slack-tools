{
    "name": "slack-tools",
    "version": "1.0.0",
    "description": "Tools for interacting with Slack's local data",
    "type": "module",
    "bin": {
        "slack-tools": "./dist/src/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "start": "tsx src/cli.ts",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:typecheck": "vitest typecheck",
        "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --ignore-pattern \"dist/**\"",
        "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --ignore-pattern \"dist/**\" --fix",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
        "mcp:inspector": "npx @modelcontextprotocol/inspector \"npm run start -- mcp -l\""
    },
    "keywords": [
        "slack",
        "tokens"
    ],
    "author": "rygwdn",
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.10.1",
        "@slack/web-api": "^7.0.1",
        "commander": "^13.1.0",
        "keytar": "^7.9.0",
        "level": "^9.0.0",
        "sqlite": "^5.1.1",
        "sqlite3": "^5.1.7",
        "zod": "^3.24.3"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.0",
        "@modelcontextprotocol/inspector": "^0.10.2",
        "@types/node": "^22.14.1",
        "@types/sqlite3": "^3.1.11",
        "@vitest/coverage-v8": "^3.1.1",
        "eslint": "^9.25.0",
        "eslint-config-prettier": "^10.1.2",
        "happy-dom": "^17.4.4",
        "prettier": "^3.5.3",
        "tsx": "^4.7.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.30.1",
        "vitest": "^3.1.1"
    },
    "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
