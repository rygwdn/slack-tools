{
  "name": "slack-tools",
  "version": "1.0.3",
  "description": "Tools for interacting with Slack's local data",
  "bin": {
    "slack-tools": "dist/cli.js"
  },
  "scripts": {
    "start": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:typecheck": "vitest typecheck",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "prebuild": "tsc --noEmit --skipLibCheck",
    "build": "tsup",
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --ignore-pattern \"dist/**\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"test/**/*.ts\" --ignore-pattern \"dist/**\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "keywords": [
    "slack",
    "tokens"
  ],
  "author": "rygwdn",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@modelcontextprotocol/sdk": "^1.10.1",
    "@slack/web-api": "^7.0.1",
    "@types/node": "^22.14.1",
    "@types/sqlite3": "^3.1.11",
    "@vitest/coverage-v8": "^3.1.1",
    "commander": "^13.1.0",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "keytar": "^7.9.0",
    "level": "^10.0.0",
    "prettier": "^3.5.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tsup": "^8.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1",
    "zod": "^3.24.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "packageManager": "pnpm@8.15.4"
}
